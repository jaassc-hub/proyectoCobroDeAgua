{
  "version": 2,
  "builds": [
    {
      "src": "tu_proyecto/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "tu_proyecto/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "tu_proyecto.settings"
  }
}